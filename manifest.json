{
    "manifest_version": 2,
    "name": "Darkside GAS",
    "version": "2.0",
    "description": "GASのスクリプトエディターの色を反転しダークモードにします。",
    "permissions": [],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://script.google.com/*/edit*"
            ],
            "css": [
                "darkside.css"
            ]
        }
    ]
}